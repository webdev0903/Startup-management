<% if skill %>
	<div class="btn btn-info">
		<%= link_to skill_starters_path(keyword.url) do %>
			<%= keyword.title %>
		<% end %>
	</div>
<% else %>
	<div class="btn btn-info btn-sm">
		<%= link_to market_startups_path(keyword.url) do %>
			<%= keyword.title %>
		<% end %>
	</div>
<% end %>