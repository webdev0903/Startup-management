}

.added_keyword .params {
    clear: both;
    font-weight: normal;
    color: rgb(160, 160, 160);
}

.remove_keyword {
    cursor: pointer;
}

/* Keywords in Right menu */
.keywordsRightMenu {
    margin-left: 22px;
    width: 218px;
}

.keywordsRightMenu input[type=text] {
    width: 119px;
    float: left;
    margin-right: 10px;
}

.keywordsRightMenu input[type=submit] {
    margin-bottom: 2px;
    padding: 7px 7px 6px 7px;
    float: left;
}

.keywordsRightMenu div#keywords_list {
    padding: 8px 0px 5px 0px;
}

/*

	Friend inputs

*/
#friend_suggestions {
    display: none;
    position: absolute;
    padding: 0px;
    width: 240px;
    height: auto;
    background: white;
    border: 1px solid rgb(230, 230, 230);
}

.friend_field {
    padding: 5px 8px 5px 8px;
    cursor: pointer;
    color: rgb(90, 90, 90);
    line-height: 100%;
}

#friends_list {
    margin-top: 4px;
}

.friend_field_inside {
    display: table;
    width: 200px;
}

.friend_field:first-child {
    border-top: none;
}

.friend_field:hover {
    background: rgb(245, 245, 245);
}

.friend_field_photo {
    float: left;
}

.friend_field_photo img {
    float: left;
    width: 30px;
    border-radius: 3px;
}

.friend_field_title {
    float: left;
    margin-left: 7px;
    font-weight: bolder;
}

.added_friend {
    float: left;
    margin: 2px;
    padding: 4px 7px 4px 7px;
    border: 1px solid rgb(230, 230, 230);
    display: table;
    font-weight: bolder;
    white-space: nowrap;
    color: rgb(90, 90, 90);
    background: rgb(255, 255, 255);
}

.added_friend_photo {
    float: left;
}

.added_friend_photo img {
    width: 30px;
}

.added_friend_title {
    margin-left: 6px;
    float: left;

}

/* links */
.remove_friend div {
    width: 40px;
    display: inline;
}

.remove_friend {
    color: #77aaca;
}

.remove_friend:hover {
    color: #3e80aa;
}

.remove_friend:hover {
    font-weight: bolder;
}

.remove_friend {
    cursor: pointer;
}

/*

	Badges

*/
.badges .row {
    margin-bottom: 5px;
}

.badgeDiv {
    padding: 4px 10px 4px 10px;
    background: #ebf9ee;
    font-size: 14px;
    font-weight: bolder;
    border: 1px solid #cfefcb;
    display: table;
    color: #07b710;
}

/*

	Question Tab 
*/
.questionTab {
    width: 600px;
    display: table;
}

.questionTabPhoto {
    float: left;
    width: 50px;
}

.questionTabPhoto img {
    width: 50px;
}

.questionTabText {
    float: left;
    width: 538px;
    margin-left: 12px;
}

.questionTabTitle {
    font-size: 15px;
    font-weight: bolder;
    color: rgb(90, 90, 90);
    margin-bottom: 3px;
}

.questionTabLinks {
    margin-top: 10px;
    color: rgb(160, 160, 160);
}

.questionTabLinks a {
    color: rgb(140, 140, 140);
}

.answersAreaTitle {
    margin-top: 15px;
    margin-bottom: 12px;
    font-size: 20px;
    font-weight: 700;
    color: rgb(100, 100, 100);
    text-shadow: 0px 1px white;
}

.answerTab {
    margin-bottom: 15px;
    padding-bottom: 15px;
    display: table;
    width: 600px;
}

.answerTabPhoto {
    float: left;
    width: 50px;
}

.answerTabPhoto img {
    width: 50px;
}

.answerTabText {
    float: left;
    margin-left: 12px;
    width: 538px;
}

.answerTabVotes {
    font-size: 15px;
    margin-bottom: 3px;
    font-weight: bolder;
    color: rgb(90, 90, 90);
}

.answerTabUp {
    float: right;
    margin-left: 1px;
    margin-bottom: 10px;
}

.answerTabDown {
    float: right;
    margin-left: 10px;
    margin-bottom: 10px;
}

.answerTabUp.voted, .answerTabDown.voted {
    opacity: 0.4;
}

#answerGiveGemEl {
    height: 18px;
    width: 24px;
    float: right;
    padding: 0px 0px 10px 10px;
}

#answerGiveGem {
    cursor: pointer;
}

