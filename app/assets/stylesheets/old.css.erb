/*

	Html tags style

*/
* {
    padding: 0px;
    margin: 0px;
}

input, button, select, textarea, label {
    /*color: rgb(70,70,70);*/
}

body {
    /*
    font-size:14px;
    color: #ffffff;
    font-family: Lato, 'Helvetica Neue', Helvetica, Arial, sans-serif;
    */
}

a {
    text-decoration: none;
}

a:hover {
}

a:link img, a:visited img {
    border: 0px;
    text-decoration: none;
}

h1 {
    /*font-size:180%; font-weight:normal; color:#ffffff;*/
}

h2 {
    /*font-size:150%;*/
}

ul {
    margin: 0px;
    padding: 0px;
}

ul li {
    list-style: none;
    margin: 0px;
    padding: 0px;
}

input:focus, textarea:focus, select:focus {
    outline: none;
}

textarea {
    /*font-family: Lato, 'Helvetica Neue', Helvetica, Arial, sans-serif;*/
    font-size: 14px;
    color: #777;
}

textarea {
    resize: none;
}

textarea.form-control {
    height: 100px;
    resize: vertical;
}

input, textarea {
    padding: 5px;
    border: 2px solid rgb(230, 230, 230);
    font-size: 15px;
    color: #2b3e50;
}

select {
    padding: 4px;
    border: none;
    color: #000000;
}

input[type=checkbox] {
    padding: 3px;
    border: none;
    border-radius: none;
    -webkit-border-radius: none;
    -moz-border-radius: none;
}

/* for privacy & terms */
ol li {
    margin-left: 40px;
    margin-top: 10px;
}

/*

	Form tags

*/
.btnMiddle {
    border: none;
    cursor: pointer;
    padding: 6px 14px 3px 14px;
    margin: 0px;
    color: white;
    font-size: 12px;
    font-weight: bolder;
    display: table;
}

.btnMiddle.green, .btnMiddle.green:hover {
    background: #27b120;
    border-top: none;
    border-left: 1px solid #259b1e;
    border-right: 1px solid #259b1e;
    border-bottom: 1px solid #259b1e;
    box-shadow: 0px 1px #20901a;
    color: white;
}

.btnMiddle.btnGray {
    border: none;
    cursor: pointer;
    background: rgb(225, 225, 225);
    font-weight: bolder;
    color: rgb(110, 110, 110);
    margin: 0px;
    border: none;
    border-right: 1px solid rgb(200, 200, 200);
    border-bottom: 1px solid rgb(200, 200, 200);
}

.btnMiddle.btnGray:hover {
    background: rgb(215, 215, 215);
    color: rgb(50, 50, 50);
}

.btnMiddle.btnGreen {
    background: #26b120;
    border-right: 1px solid #239c1d;
    border-bottom: 1px solid #239c1d;
}

.btnMiddle.btnGreen:hover {
    color: white;
    background: #1eaa18;
    border-right: 1px solid #168e10;
    border-bottom: 1px solid #168e10;
}

/*
	Round
*/
.btnMiddle.btnRound {
    padding: 0px 10px 0px 10px;
}

